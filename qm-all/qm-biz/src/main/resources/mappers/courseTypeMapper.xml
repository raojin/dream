<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qmdj.dao.course.CourseTypeDAO">

	<resultMap type="CourseType" id="courseTypeRemappering">
	<result column="id" property="id" />
	<result column="gmt_create" property="gmtCreate"/>
	<result column="gmt_modified" property="gmtModified"/>
	<result column="name" property="name"/>
	<result column="status" property="status"/>
	<result column="type" property="type"/>
	</resultMap>
           
     <insert id="saveCourseType" parameterType="CourseType" useGeneratedKeys="true" keyProperty="id">
         <selectKey resultType="long" order="AFTER" keyProperty="id">
            SELECT LAST_INSERT_ID()
        </selectKey>
         insert into qmdj_course_type(gmt_modified,gmt_create,name,status,type) 
        values (now(),now(),#{name},#{status},#{type})
      </insert>
      
      <select id="queryCourseTypeList" resultMap="courseTypeRemappering">
       select * from qmdj_course_type where status=1
      </select>
      
</mapper>